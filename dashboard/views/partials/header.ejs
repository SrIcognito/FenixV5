<html lang="en">

<head>
  <meta data-n-head="ssr" charset="utf-8" name="viewport" content="width=device-width, initial-scale=1">

  <!-- Head Information -->
  <title>
    <%= title %>
  </title>
  <link data-n-head="ssr" rel="icon" type="image/x-icon" href="/assets/RedFenixFavi.ico">

  <!-- Styles -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link type="text/css" rel="stylesheet" href="/styles/bootstrap.min.css">
  <link type="text/css" rel="stylesheet" href="/styles/utilities.css">
  <link type="text/css" rel="stylesheet" href="/styles/common.css">
  <link type="text/css" rel="stylesheet" href="/styles/index.css">
  <link id="Style" type="text/css" rel="stylesheet">

  <!-- Arc -->
  <script async src="https://arc.io/widget.min.js#bQxm15Ww"></script>
  
  <!-- Font Awesome & Boxicons-->
  <link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
  <link href="https://pro.fontawesome.com/releases/v5.15.1/css/all.css" rel="stylesheet">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" integrity="sha512-+4zCK9k+qNFUR5X+cKL9EIR+ZOhtIloNl9GIKS57V1MyNsYpYcUrUeQc9vNfzsWfV28IaLL3i96P9sdNyeRssA==" crossorigin="anonymous">

  <!-- Animate.css -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css">

  <!-- Meta -->
  <meta data-n-head="ssr" data-hid="theme-color" name="theme-color" content="#ff5454">
  <meta data-n-head="ssr" data-hid="og:type" name="og:type" content="website">
  <meta data-n-head="ssr" data-hid="og:title" name="og:title" content="RedFenix - Multi-Lenguaje Discord Bot!">
  <meta data-n-head="ssr" data-hid="og:url" name="og:url" content="https://fenixdashdiscord.com">
  <meta data-n-head="ssr" data-hid="og:image" name="og:image" content="/assets/RedFenix.png">
  <meta data-n-head="ssr" data-hid="description" name="description"
    content="RedFenix es un Discord Bot dedicado a la moderaci贸n y la diversi贸n en varios idiomas. Creado con amor para mantener ocupados y activos a sus miembros Servidor.">
  <meta data-n-head="ssr" data-hid="og:description" name="og:description"
    content="RedFenix es un Discord Bot dedicado a la moderaci贸n y la diversi贸n en varios idiomas. Creado con amor para mantener ocupados y activos a sus miembros Servidor.">
</head>

<body>

  <!-- ScrollTop -->
  <a onclick="ScrollUp();" class="button scrollTop" id="scroll-top">
    <i class="fas fa-arrow-up"></i>
  </a>

  <!-- Preloader -->
  <div class="preloader" id="Preloader">
    <div class="lds-grid">
      <div></div>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
    </div>
  </div>

  <div id="myModal" class="modal">
    <div class="center modal-box">
      <div class="fas fa-times"></div>
      <img src="assets/RedFenix.png" alt="RedFenix" class="icon1">
      <header>RedFenix Premium</header>
      <p>
        Con un Codigo Valido podras reclamar los Beneficios Premium de RedFenix!
      </p>
      <form action="/premium" method="post" onsubmit="return false">
        <div class="field code">
          <div class="input-area">
            <input type="text" id="code" name="code" placeholder="Codigo">
            <i class="icon fas fa-money-check-alt"></i>
            <i class="error error-icon fas fa-exclamation-circle"></i>
          </div>
          <span class="error error-txt"></span>
          <span class="success success-txt"></span>
        </div>
        <button type="button" class="submit">Reclamar</button>
      </form>
    </div>
  </div>

  <!-- Header -->
  <div class="hide" id="Content">
    <nav class="navbar">
      <div class="container full flex">
        <a href="/" class="logo flex">
          <img src="assets/RedFenix.png" alt="RedFenix">
          <p><b>RedFenix</b></p>
        </a>
        <div class="flex">
          <ul class="NavbarItems">
            <li><a class="button nav-item" href="/"><span>Casa</span></a></li>
            <li><a class="button" href="commands"><span>Comandos</span></a></li>
            <li><a class="button" href="information"><span>Informacion</span></a></li>
          </ul>
          <% if (user) { %>
            <div class="dropdown">
              <div id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
                <%- user.avatar ? `<img width="40px" oncontextmenu="return false" draggable="false"  src="https://cdn.discordapp.com/avatars/${user.id}/${user.avatar}" class="mr-2" alt="avatar">` : `<img width="40px" oncontextmenu="return false" draggable="false"  src="/assets/DefaultAvatar.png" class="mr-2" alt="avatar">` %>
              </div>                
              <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="dropdownMenuButton1">
                  <div class="dropdown-item" id="dash">
                    <a href="/dashboard">
                      <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="stream" role="img"
                        xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"
                        class="svg-inline--fa fa-stream fa-w-16 fa-2x">
                        <path fill="currentColor"
                          d="M16 128h416c8.84 0 16-7.16 16-16V48c0-8.84-7.16-16-16-16H16C7.16 32 0 39.16 0 48v64c0 8.84 7.16 16 16 16zm480 80H80c-8.84 0-16 7.16-16 16v64c0 8.84 7.16 16 16 16h416c8.84 0 16-7.16 16-16v-64c0-8.84-7.16-16-16-16zm-64 176H16c-8.84 0-16 7.16-16 16v64c0 8.84 7.16 16 16 16h416c8.84 0 16-7.16 16-16v-64c0-8.84-7.16-16-16-16z"
                          class=""></path>
                      </svg>
                      <span>Dashboard</span>
                    </a>
                  </div>
                  <div class="dropdown-item">
                    <a class="cursor-pointer" id="myBtn">
                      <svg aria-hidden="true" focusable="false" data-prefix="fad" data-icon="fire-alt" role="img"
                        xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"
                        class="svg-inline--fa fa-fire-alt fa-w-14 fa-3x">
                        <g class="fa-group">
                          <path fill="currentColor"
                            d="M323.56 51.2a597.38 597.38 0 0 0-56.22 60C240.08 73.62 206.28 35.53 168 0 69.74 91.17 0 210 0 281.6 0 408.85 100.29 512 224 512s224-103.15 224-230.4c0-53.27-52-163.14-124.44-230.4zm-1.12 366.87A165.81 165.81 0 0 1 226.86 448c-43.93 0-84.43-14.89-114.06-41.92a146.18 146.18 0 0 1-35.88-50.39C68.35 335.82 64 314 64 290.75c0-59.43 42.8-106.39 104.3-180.12 30 34.59 18.49 19.78 100.7 124.59l62-70.74c24.32 40.25 27.78 45.59 34.84 59.1a157.93 157.93 0 0 1 15 104.62c-7.49 36.85-28.24 68.8-58.4 89.87z"
                            class="fa-secondary"></path>
                          <path fill="currentColor"
                            d="M304.09 391.85A134.39 134.39 0 0 1 226.86 416C154.71 416 96 368.26 96 290.75c0-38.61 24.31-72.63 72.79-130.75 6.93 8 98.83 125.34 98.83 125.34l58.63-66.88c4.14 6.85 7.91 13.55 11.27 20 27.35 52.19 15.81 119-33.43 153.42z"
                            class="fa-primary"></path>
                        </g>
                      </svg> <span>Premium</span>
                    </a>
                  </div>
                  <div class="dropdown-item">
                    <a href="/logout" class="cursor-pointer text-danger">
                      <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                        stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                        <path d="M9 21H5a2 2 0 0 1-2-2V5a2 2 0 0 1 2-2h4"></path>
                        <polyline points="16 17 21 12 16 7"></polyline>
                        <line x1="21" y1="12" x2="9" y2="12"></line>
                      </svg>
                      <span>Log Out</span>
                    </a>
                  </div>
                </ul>
            </div>
            <% } else { %>
              <a href="/login"><button class="button btn2 desktop"><i class="fas fa-sign-in-alt"></i>
                  Login</button></a>
              <% } %>
        </div>
      </div>
    </nav>